#include <stdio.h>

int main () {

    int numero, i;

    // mover a torre 5 casas para a direita
for (int i = 0; i < 5; i++) {
    printf("Direita\n"); // Imprime a direção do movimento 
}

// mover a rainha 8 casas para a esquerda

while ( i <= 8)

{
    printf("Esquerda\n "); // Imprime a direção do movimento
    i++;
     
}

return 0;

}